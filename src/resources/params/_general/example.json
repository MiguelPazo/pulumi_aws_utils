[
  {
    "name": "/database/host",
    "type": "String",
    "value": "db.{{stack}}.{{region}}.rds.amazonaws.com",
    "description": "Database host endpoint"
  },
  {
    "name": "/database/port",
    "type": "String",
    "value": "5432",
    "description": "Database port"
  },
  {
    "name": "/api/key",
    "type": "SecureString",
    "value": "your-api-key-here",
    "description": "API key for external service",
    "ignoreChanges": true
  },
  {
    "name": "/aws/account-id",
    "type": "String",
    "value": "{{accountId}}",
    "description": "AWS Account ID"
  },
  {
    "name": "/aws/region",
    "type": "String",
    "value": "{{region}}",
    "description": "AWS Region"
  },
  {
    "name": "/app/prefix",
    "type": "String",
    "value": "{{generalPrefix}}",
    "description": "General prefix for resources"
  },
  {
    "name": "/config/failover",
    "type": "SecureString",
    "value": "[[failover.json]]",
    "description": "Multi-region failover configuration for Step Functions",
    "ignoreChanges": true
  }
]
